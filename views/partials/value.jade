td
  - if (typeof value !== 'undefined')
    - if (typeof value === 'object')
      div.flot-series-year
      - for (year in value)
        div.graph
        div.data
          span.year= year
          span.value= value[year]
          span.display= format(value[year], schema.properties[field].format)
    - else
      div.data= format(value, schema.properties[field].format)
